# Use $EDITOR for vi-inspired editors
alias nvim="$EDITOR"
alias vim="$EDITOR"
alias vi="$EDITOR"

# use Python 3 by default
alias python2="command python"
alias python="python3"
alias pip2="command pip"
alias pip="pip3"

# other
alias sudo='sudo -E'
alias more="less"
alias :q="exit"
alias re="fc -s"

